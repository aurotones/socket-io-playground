{
  "name": "socket-io-playground",
  "version": "0.1.21",
  "description": "Socket.io client playground for developers",
  "main": "./out/main/index.js",
  "author": "Uuganbayar Otgonbayar <aurotones@gmail.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aurotones/socket-io-playground.git"
  },
  "keywords": [
    "socket.io",
    "socket",
    "ws",
    "playground"
  ],
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config --publish never",
    "build:mac": "electron-vite build && electron-builder --mac --config --publish never",
    "build:linux": "electron-vite build && electron-builder --linux --config --publish never",
    "publish:win-x86": "electron-vite build && electron-builder --win --x64 --config --publish onTagOrDraft",
    "publish:win-arm": "electron-vite build && electron-builder --win --arm64 --config --publish onTagOrDraft",
    "publish:mac-x86": "electron-vite build && electron-builder --mac --x64 --config --publish onTagOrDraft",
    "publish:mac-arm": "electron-vite build && electron-builder --mac --arm64 --config --publish onTagOrDraft",
    "publish:linux-x86": "electron-vite build && electron-builder --linux --x64 --config --publish onTagOrDraft",
    "publish:linux-arm": "electron-vite build && electron-builder --linux --arm64 --config --publish onTagOrDraft"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.0.0",
    "@codemirror/lang-json": "^6.0.1",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@uiw/codemirror-themes": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.0.0",
    "electron-updater": "^6.1.1",
    "lucide-react": "^0.303.0",
    "moment": "^2.30.1",
    "nanoevents": "^9.0.0",
    "query-string": "^8.1.0",
    "react-github-btn": "^1.4.0",
    "react-ink": "^6.5.4",
    "react-json-editor-ajrm": "^2.5.14",
    "react-redux": "^9.0.4",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "url-parse": "^1.5.10",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-json-editor-ajrm": "^2.5.6",
    "@types/url-parse": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "np": "^9.2.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
